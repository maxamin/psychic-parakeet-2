<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/colorBlueGray"
    tools:context=".ChallengeList"
    android:id="@+id/relativeLayout2">

    <TextView
        android:id="@+id/textViewChall"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="80dp"
        android:text="Challenges"
        android:textColor="@color/colorWhite"
        android:textSize="20dp"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent" />

    <Button
        android:id="@+id/buttonlevel1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        style="@style/Widget.AppCompat.Button.Colored"
        android:theme="@style/buttontheme"
        android:background="@drawable/button"
        android:layout_marginStart="25dp"
        android:layout_marginTop="139dp"
        android:text="LEVEL1"
        tools:ignore="RtlCompat"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

    <Button
        android:id="@+id/buttonlevel2"
        style="@style/Widget.AppCompat.Button.Colored"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="LEVEL2"
        tools:ignore="RtlCompat"
        android:background="@drawable/button"
        app:layout_constraintTop_toTopOf="@+id/buttonlevel1"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent" />

    <Button
        android:id="@+id/buttonlevel3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        style="@style/Widget.AppCompat.Button.Colored"
        android:layout_marginEnd="24dp"
        android:text="LEVEL3"
        tools:ignore="RtlCompat"
        android:background="@drawable/button"
        app:layout_constraintTop_toTopOf="@+id/buttonlevel1"
        app:layout_constraintEnd_toEndOf="parent" />

    <Button
        android:id="@+id/buttonlevel4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:background="@drawable/button"
        android:layout_marginTop="209dp"
        style="@style/Widget.AppCompat.Button.Colored"
        android:text="LEVEL4"
        tools:ignore="RtlCompat"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintStart_toStartOf="@+id/buttonlevel1" />

    <Button
        android:id="@+id/buttonlevel5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:background="@drawable/button"
        android:text="LEVEL5"
        style="@style/Widget.AppCompat.Button.Colored"
        tools:ignore="RtlCompat"
        app:layout_constraintTop_toTopOf="@+id/buttonlevel4"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent" />

    <TextView
        android:id="@+id/textViewuser"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textSize="30dp"
        android:textColor="@color/colorWhite"
        android:layout_marginTop="24dp"
        android:text=""
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent" />

    <Button
        android:id="@+id/buttonlevel6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="LEVEL6"
        tools:ignore="RtlCompat"
        android:background="@drawable/button"
        style="@style/Widget.AppCompat.Button.Colored"
        app:layout_constraintTop_toTopOf="@+id/buttonlevel4"
        app:layout_constraintStart_toStartOf="@+id/buttonlevel3" />

    <Button
        android:id="@+id/buttonlevel7"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="185dp"
        android:background="@drawable/button"
        style="@style/Widget.AppCompat.Button.Colored"
        android:text="LEVEL7"
        tools:ignore="RtlCompat"
        app:layout_constraintStart_toStartOf="@+id/buttonlevel1"
        app:layout_constraintBottom_toBottomOf="parent" />

    <Button
        android:id="@+id/buttonlevel8"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        style="@style/Widget.AppCompat.Button.Colored"
        android:background="@drawable/button"
        android:text="LEVEL8"
        app:layout_constraintTop_toTopOf="@+id/buttonlevel7"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent" />

    <Button
        android:id="@+id/buttonlevel9"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        style="@style/Widget.AppCompat.Button.Colored"
        android:background="@drawable/button"
        android:text="LEVEL9"
        app:layout_constraintTop_toTopOf="@+id/buttonlevel7"
        app:layout_constraintStart_toStartOf="@+id/buttonlevel3"
        app:layout_constraintLeft_toLeftOf="@+id/buttonlevel3" />

    <Button
        android:id="@+id/buttonlevel10"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="LEVEL10"
        android:background="@drawable/button"
        tools:ignore="RtlCompat"
        style="@style/Widget.AppCompat.Button.Colored"
        app:layout_constraintTop_toTopOf="@+id/buttonlevel11"
        app:layout_constraintStart_toStartOf="@+id/buttonlevel1"
        app:layout_constraintLeft_toLeftOf="@+id/buttonlevel1" />

    <Button
        android:id="@+id/buttonlevel11"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="110dp"
        android:background="@drawable/button"
        android:text="LEVEL11"
        app:layout_constraintBottom_toBottomOf="parent"
        style="@style/Widget.AppCompat.Button.Colored"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent" />

    <Button
        android:id="@+id/buttonlevel12"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="LEVEL12"
        android:background="@drawable/button"
        tools:ignore="RtlCompat"
        style="@style/Widget.AppCompat.Button.Colored"
        app:layout_constraintTop_toTopOf="@+id/buttonlevel10"
        app:layout_constraintStart_toStartOf="@+id/buttonlevel3"
        app:layout_constraintLeft_toLeftOf="@+id/buttonlevel3" />

</android.support.constraint.ConstraintLayout>